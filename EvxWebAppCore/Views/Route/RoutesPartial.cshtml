@using EvxWebAppCore.Models
@model List<RouteModel>
    <div class="scrollable">
        @foreach (RouteModel entry in Model)
        {
            <div class="button-list-default-margin">
                <button type="button" class="btn btn-secondary btn-lg btn-block" onclick="Manage('Stations',@(entry.RouteID),'@(entry.RouteName)')">@entry.RouteName</button>
            </div>
        }
    </div>